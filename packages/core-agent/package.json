{
  "name": "@ai-agent-mastery/core-agent",
  "version": "1.0.0",
  "description": "Core AI agent implementation with Mastra",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@ai-agent-mastery/shared": "workspace:*",
    "@mastra/core": "^0.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "openai": "^4.77.0",
    "@anthropic-ai/sdk": "^0.32.1",
    "groq-sdk": "^0.8.0",
    "vm2": "^3.9.19",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1",
    "eslint": "^8.56.0"
  }
}
